# Anthropic API key for Claude access
# Get one at https://console.anthropic.com/
ANTHROPIC_API_KEY=your-anthropic-api-key-goes-here

# Port for the proxy server to listen on
PORT=3000

# Environment (optional, leave empty to use Vercel AI SDK)
NODE_ENV=

# Optional: Enable verbose logging (true/false)
DEBUG=false

# Optional: Timeout for Anthropic requests in milliseconds
ANTHROPIC_TIMEOUT=10000